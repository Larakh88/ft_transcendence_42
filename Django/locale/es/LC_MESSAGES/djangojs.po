#: pong/static/pong/js/game.js:25
msgid "Please enter a value for Player 2 Alias!"
msgstr "¡Por favor, ingrese un valor para el Alias del Jugador 2!"

#: pong/static/pong/js/game.js:29
msgid "Alias is between 3 and 10 characters!"
msgstr "¡El alias debe tener entre 3 y 10 caracteres!"

#: pong/static/pong/js/game.js:33 pong/static/pong/js/multiGame.js:28
msgid "Alias cannot contain spaces!"
msgstr "¡El alias no puede contener espacios!"

#: pong/static/pong/js/game.js:39
msgid "Alias cannot be the same as the user!"
msgstr "¡El alias no puede ser igual al usuario!"

#: pong/static/pong/js/game.js:191 pong/static/pong/js/multiGame.js:194
msgid "Game Over!<br>"
msgstr "Fin del juego!<br>"

#: pong/static/pong/js/game.js:191 pong/static/pong/js/multiGame.js:194
msgid " Wins"
msgstr "Victorias"

#: pong/static/pong/js/multiGame.js:20
msgid "Please enter all aliases"
msgstr "Por favor, ingrese todos los alias"

#: pong/static/pong/js/multiGame.js:24
msgid "Alias cannot be more than 10 characters!"
msgstr "¡El alias no puede tener más de 10 caracteres!"

#: pong/static/pong/js/multiGame.js:32
msgid "Aliases cannot be identical!"
msgstr "¡Los alias no pueden ser idénticos!"

#: pong/static/pong/js/multiGame.js:194
msgid "Tie"
msgstr "Empate"

#: pong/static/pong/js/multiGame.js:194
msgid "It's a Tie"
msgstr "Es un Empate"

